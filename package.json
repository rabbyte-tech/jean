{
  "name": "jean2",
  "version": "0.1.0",
  "private": true,
  "description": "Jean2 - AI Agent Monorepo",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "bun run dev:server & bun run dev:client",
    "dev:server": "cd packages/server && bun --watch --env-file=../../.env src/index.ts",
    "dev:client": "cd packages/client && bun run dev",
    "build": "bun run --filter '*' build",
    "typecheck": "bun run --filter '*' typecheck",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "eslint": "^10.0.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.56.1"
  }
}
